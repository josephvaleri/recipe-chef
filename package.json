{
  "name": "recipe-chef",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:web": "next dev --turbopack -p 3000",
    "build": "next build --turbopack",
    "build:web": "next build --turbopack",
    "build:mobile": "mv src/middleware.ts src/middleware.web.ts 2>/dev/null || true && NEXT_CONFIG_FILE=next.config.mobile.js next build && node scripts/export-mobile.js && mv src/middleware.web.ts src/middleware.ts 2>/dev/null || true",
    "export:mobile": "npm run build:mobile",
    "start": "next start",
    "lint": "eslint",
    "test:e2e": "playwright test",
    "cap:init": "npx cap init RecipeChef com.recipechef.app --web-dir=out",
    "cap:sync": "npx cap sync",
    "cap:open:ios": "npx cap open ios",
    "cap:open:android": "npx cap open android",
    "cap:add:ios": "npx cap add ios",
    "cap:add:android": "npx cap add android",
    "cap:assets": "npx capacitor-assets generate --platform ios --platform android"
  },
